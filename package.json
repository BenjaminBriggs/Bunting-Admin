{
  "name": "bunting-admin",
  "version": "0.1.0",
  "description": "Bunting feature flag management admin interface",
  "private": true,
  "overrides": {
    "test-exclude": "7.0.1",
    "babel-plugin-istanbul": {
      "test-exclude": "7.0.1"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:open": "npm run test:cov && open coverage/lcov-report/index.html",
    "test:ci": "jest --coverage --runInBand --selectProjects unit",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:json-spec": "jest tests/json-spec-validation.test.js --verbose",
    "test:e2e": "playwright test",
    "e2e:build": "next build",
    "e2e:start": "next start -p 3000",
    "e2e": "start-server-and-test e2e:start http://localhost:3000 'playwright test'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm run docker:up && npm run setup:wait && npm run db:push",
    "setup:wait": "echo 'Waiting for services to be ready...' && sleep 10 && docker wait bunting-minio-init || echo 'MinIO initialization complete'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@faker-js/faker": "^10.0.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/material-nextjs": "^7.3.2",
    "@mui/x-data-grid": "^8.12.0",
    "@prisma/client": "^6.16.2",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "jose": "^5.1.0",
    "next": "^15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "node-jose": "^2.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-jest": "^30.1.2",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "msw": "^2.11.3",
    "prettier": "^3.0.0",
    "prisma": "^6.16.2",
    "rimraf": "^5.0.10",
    "start-server-and-test": "^2.1.2",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=24 <25"
  }
}
